#tdlib.h>
#include <time.h>
/* more headers goes there */

/* betty style doc for function main goes there */
int main(void)
{
  int n;

  srand(time(0));
  n = rand() - RAND_MAX / 2;
  if ( %10n > 5)
    {
      printf ("Last digit of %d is %10d and is greater than 5 ");
    }
  else if (%10n == 0)
    {
      printf ("Last digit of %d is %10d and is 0");
    }
  else
    {
      printf ("Last digit of %d is %10d and is less than 6 and not 0");
    }
  return (0);
